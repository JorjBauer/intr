CC=@CC@
ALL=intr
CFLAGS=@CFLAGS@
LDFLAGS=@LDFLAGS@

SRC=intr.c version.c

all:	intr

intr:	intr.o version.o
	${CC} ${CFLAGS} ${LDFLAGS} -o intr intr.o version.o

nointr:	nointr.o
	${CC} ${CFLAGS} ${LDFLAGS} -o nointr nointr.o

test:
	./intr 5 ./nointr

clean:
	rm -f *.o *~ intr nointr

distclean: clean
	rm -f config.log config.status config.cache Makefile version.c
	rm -rf autom4te.cache
